# .env for Staging Environment

# for docker compose
COMPOSE_PROJECT_NAME=capstone-staging

# web
WEB_PORT=3000
VITE_API_BASE_URL=https://api-staging.yourdomain.com

# api
API_PORT=4000
NODE_ENV=staging

CORS_ORIGIN=https://staging.yourdomain.com
PRODUCTION_CORS_ORIGIN=https://staging.yourdomain.com

DB_TYPE=mysql
DB_ORM=typeorm
DATABASE_URL=mysql://staging_user:staging_password@staging-db:3306/cms_staging
ALLOW_PRODUCTION_AUTO_MIGRATION=true